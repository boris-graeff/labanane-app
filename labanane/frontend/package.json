{
  "name": "labanane",
  "description": "LaBanane frontend app",
  "author": "Boris Graeff",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "xo; ava test"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "soundcloud": "^3.1.2",
    "vue": "^2.0.1",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.0.2",
    "vuex": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "nock": "^9.0.2",
    "node-sass": "^3.13.0",
    "sass-loader": "^4.0.2",
    "vue-loader": "^9.7.0",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-server": "^2.1.0-beta.12",
    "xo": "^0.17.1"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "browser"
    ],
    "ignores": [
      "webpack.config.js"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "comments": false,
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
